{
  "manifest_version": 2,
  "name": "Open in VSCode",
  "version": "1.0",

  "description": "Clone and open git repos from GitHub in VS Code.",

  "icons": {
    "48": "icons/vscode.svg",
    "96": "icons/vscode.svg"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*/*"],
      "js": ["openInVSCode.js"]
    }
  ]
}
