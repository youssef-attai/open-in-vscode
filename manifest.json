{
  "manifest_version": 3,
  "name": "Open in VSCode",
  "version": "1.0",
  "description": "Clone and open git repos from GitHub in VS Code.",
  "icons": {
    "16": "icons/vscode-16.png",
    "32": "icons/vscode-32.png",
    "48": "icons/vscode-48.png",
    "64": "icons/vscode-64.png",
    "96": "icons/vscode-96.png",
    "128": "icons/vscode-128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*/*"],
      "js": ["openInVSCode.js"]
    }
  ]
}
